# Required environment variables
OPENAI_API_KEY=your_openai_api_key_here

# Optional: LangSmith integration
# If you want to use LangSmith for tracing and monitoring, uncomment and set the following
# LANGSMITH_API_KEY=your_langsmith_api_key_here
# LANGSMITH_PROJECT=your_project_name  # Optional project name

# ServiceNow integration (if using ServiceNow features)
SERVICENOW_INSTANCE=your_servicenow_instance
SERVICENOW_USERNAME=your_servicenow_username
SERVICENOW_PASSWORD=your_servicenow_password
SERVICENOW_TOKEN=your_servicenow_token
